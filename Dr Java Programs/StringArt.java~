//Jacob Allen
//This program practices Strings and escape sequences

public class StringArt
{
  public static void main(String[] args)
  {
    String line1 = "                                       .,;;;;;;;,.";
    String line2 = "                                     ,;;;;;;;,/;;;;";
    String line3 = "                    .,aa@@@@@@@@@@@@a;;;;;/;;;,//;;;";
    String line4 = "           ..,,,.,aa@@@@@@@@@@@@@@@@@@@a;//;;;,//;;;";
    String line5 = "        ,;;;;;;;O@@@@@OO@@@@@@@@@@@@@@OOO@@@a,/;;;;'";
    String line6 = "      .;;//,;;;O@@@@OOO@@@@@@@@@@OOO@@@@OOO@@@@@a'";
    String line7 = "     .;;/,;;/;OO@@OO@@@@@@@@@@@@@@@@@@@@@@@OOO@@@@.";
    String line8 = "     ;;;/,;;//OO@@@@@@@OOO@@@@@@@@@@@OOO@@@@@@@@@@@.";
    String line9 = "     `;;//,;,OOO@@@@@@@@@OO@@@@@@@@@OO@@@@@@@@@@@@@@.";  
    String line10= "   ;.  ``````OOO@@@@@;;;;;;OO@@@@@OO;;;;;;@@@@@@O@@@@.";
    String line11= "  .;;,       OOO@@@O;;' ~`;@@OOOOO@@;' ~`;;O@@@@@OO@@@";
    String line12= "  ;;;;    ,  OOO@@O;;;,.,;O@@@@@@@@@O;,.,;;;O@@@@OO@@@,";
    String line13= "  `;;'   ,;; OOO@@OO;;;;OOO(???????)OOO;;;;OO@@@@OO@@@%,";
    String line14= "    `\   ;;; `OOO@@@@@OOOO@@\?????/@@OOOO@@@@@@@O@@@@%O@a";
    String line15= "       \,`;'  `OOO@@@@OOO@@@@@@;@@@@@@OOO@@@@@@@@@@@%O@@@,";
    String line16= "       .,\      `OO@@@@OO''@@@@@;@@@@@OO@@@@@@@@@@%oO@@@O@;";
    String line17= "     ,;;;; \   .::::OO@@OOOaaa@@@aaaOOO@@@@@@@',;OO@@OOO@@;,";
    String line18= "    .;;''    \:::.OOaa`@@@OO@@@@@@@OO@@@'::aOO.:;;OO@@@OO;::.";
    String line19= "    '       .::\.OO@@@@O@::;;;;;;;;;;;;::O@O@OO.::::::::://::";
    String line20= "           .:::.O\@@@@@@@@O@O::;;;::O@OO@O@@@@OO.:;;;;;;;;//:,";
    System.out.print(line1 + "\n");
    System.out.print(line2 + "\n");
    System.out.print(line3 + "\n");
    System.out.print(line4 + "\n");
    System.out.print(line5 + "\n");
    System.out.print(line6 + "\n");
    System.out.print(line7 + "\n");
    System.out.print(line8 + "\n");
    System.out.print(line9 + "\n");
    System.out.print(line10 + "\n");
    System.out.print(line11 + "\n");
    System.out.print(line12 + "\n");
    System.out.print(line13 + "\n");
    System.out.print(line14 + "\n");
    System.out.print(line15 + "\n");
    System.out.print(line16 + "\n");
    System.out.print(line17 + "\n");
    System.out.print(line18 + "\n");
    System.out.print(line19 + "\n");
    System.out.print(line20 + "\n");
  }
}
    